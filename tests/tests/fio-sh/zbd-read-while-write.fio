[global]
bs=128k
direct=1
zonemode=zbd
name=readwhilewriting
time_based
runtime=30

[write]
filename=/dev/nvme0n1
ioengine=psync
rw=write
iodepth=1
offset_increment=1z
size=128z
numjobs=10
max_open_zones=128
flow=-3

[randread]
filename=/dev/nvme0n1
ioengine=io_uring
rw=randread
iodepth=32
size=128z
numjobs=10
max_open_zones=128
flow=7
